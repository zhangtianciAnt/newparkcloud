<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nt.service_pfans.PFANS2000.mapper.GoalManagementMapper">
    <select id="yearsCheck" parameterType="com.nt.dao_Pfans.PFANS2000.GoalManagement"
            resultType="com.nt.dao_Pfans.PFANS2000.GoalManagement">
     select * from GoalManagement
     where  years= #{years}
     and user_id= #{user_id}
    </select>

</mapper>
