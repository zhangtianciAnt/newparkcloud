<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nt.service_pfans.PFANS2000.mapper.PunchcardRecordDetailMapper">
    <delete id="deletetepunbp">
        DELETE FROM punchcardrecordbp WHERE date_format(punchcardrecord_date, '%Y-%m-%d') = #{punchcardrecord_date};
    </delete>

    <delete id="deletetepundetbp">
        DELETE FROM punchcardrecorddetailbp WHERE date_format(punchcardrecord_date, '%Y-%m-%d') = #{punchcardrecord_date};
    </delete>
</mapper>
