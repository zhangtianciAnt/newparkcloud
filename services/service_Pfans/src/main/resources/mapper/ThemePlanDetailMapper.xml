<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nt.service_pfans.PFANS1000.mapper.ThemePlanDetailMapper">
	<select id="getDetailList" resultType="com.nt.dao_Pfans.PFANS1000.ThemePlanDetail">
		select tpd.*
		from themeplandetail tpd
		where tpd.THEMEPLAN_ID = #{themeplan_id}
		order by tpd.CREATEON desc
	</select>
</mapper>
