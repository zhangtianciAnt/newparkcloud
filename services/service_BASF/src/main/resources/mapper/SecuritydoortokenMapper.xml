<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nt.service_BASF.mapper.SecuritydoortokenMapper">
    <resultMap id="BaseResultMap" type="com.nt.dao_BASF.Securitydoortoken">
        <id column="id" property="id" jdbcType="VARCHAR"/>
        <result column="token" property="token" jdbcType="VARCHAR"/>
        <result column="MODIFYON" property="modifyon" jdbcType="TIMESTAMP"/>
    </resultMap>

    <select id="getDailyInfoToken" resultType="com.nt.dao_BASF.VO.SecuritydoortokenVo">
        SELECT
            token
        FROM
            securitydoortoken
        WHERE
            id = #{id}
    </select>
</mapper>
