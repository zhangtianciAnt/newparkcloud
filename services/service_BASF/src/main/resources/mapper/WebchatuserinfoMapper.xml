<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.nt.service_BASF.mapper.WebchatuserinfoMapper">
    <resultMap id="BaseResultMap" type="com.nt.dao_BASF.Webchatuserinfo">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="loginid" property="loginid" jdbcType="VARCHAR"/>
        <result column="password" property="password" jdbcType="VARCHAR"/>
        <result column="CREATEON" property="createon" jdbcType="TIMESTAMP"/>
        <result column="CREATEBY" property="createby" jdbcType="VARCHAR"/>
        <result column="MODIFYON" property="modifyon" jdbcType="TIMESTAMP"/>
        <result column="MODIFYBY" property="modifyby" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="selectPwd" resultType="com.nt.dao_BASF.Webchatuserinfo">
		SELECT
        password
		FROM
		webchatuserinfo
        WHERE
        loginid = #{loginid}
	</select>

</mapper>
